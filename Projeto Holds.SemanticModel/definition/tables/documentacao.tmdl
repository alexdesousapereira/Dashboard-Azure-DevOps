	partition documentacao = calculated
		mode: import
		source =
				
				VAR _measures_info =
				    INFO.VIEW.MEASURES()
				VAR _measures =
				    SELECTCOLUMNS(
				        _measures_info,
				        "Type", "Measure",
				        "Name", [Name],
				        "Description", [Description],
				        "Location",
				            "Tabela: " & [Table] & UNICHAR(10) & "Pasta: " & [DisplayFolder],
				        "Expression", [Expression]
				    )
				VAR _columns =
				    SELECTCOLUMNS(
				        FILTER(
				            INFO.VIEW.COLUMNS(),
				            [Table] <> "Data Dictionary" && NOT ([IsHidden])
				        ),
				        "Type", "Column",
				        "Name", [Name],
				        "Description", [Description],
				        "Location", "Tabela: " & [Table],
				        "Expression", ""
				    )
				VAR _combined =
				    UNION(
				        _columns,
				        _measures
				    )
				RETURN
				    _combined

	annotation PBI_Id = 948435c829db435fbb8522bfc0cc7f4b

